# Supabase Configuration (from Supabase Dashboard -> Settings -> API)
SUPABASE_URL=
SUPABASE_ANON_KEY=

# Public URL for this relay service (use localhost for development, Render URL for production)
PUBLIC_URL=

# Relay DID (optional - auto-derived from PUBLIC_URL if not set)
# Format: did:web:your-domain.com
RELAY_DID=

# Server Configuration
NODE_ENV=
PORT=

# Database Configuration
DB_QUERY_TIMEOUT=10000  # Query timeout in milliseconds (default: 10 seconds)

# ZK Verification Configuration
# Base64-encoded verification key JSON, or file://path/to/key.json
ZK_VERIFICATION_KEY=
